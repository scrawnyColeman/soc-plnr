{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": "./",
    "paths": {
      "src/*": [
        "src/*"
      ],
      "atoms/*": [
        "src/components/atoms/*"
      ],
      "atoms": [
        "src/components/atoms/index.ts"
      ],
      "molecules/*": [
        "src/components/molecules/*"
      ],
      "molecules": [
        "src/components/molecules/index.ts"
      ],
      "organisms/*": [
        "src/components/organisms/*"
      ],
      "organisms": [
        "src/components/organisms/index.ts"
      ],
      "views/*": [
        "src/views/*"
      ],
      "views": [
        "src/views/index.ts"
      ],
      "wrappers/*": [
        "src/components/wrappers/*"
      ],
      "wrappers": [
        "src/components/wrappers/index.ts"
      ],
      "lib/*": [
        "./lib/*"
      ],
      "lib": [
        "./lib/index.ts"
      ],
      "hooks/*": [
        "src/hooks/*"
      ],
      "hooks": [
        "src/hooks/index.ts"
      ],
      "services/*": [
        "src/services/*"
      ],
      "services": [
        "src/services/index.ts"
      ],
      "assets/*": [
        "src/assets/*"
      ],
      "assets": [
        "src/assets/index.ts"
      ],
    }
  },
  "include": [
    "next-env.d.ts",
    "src",
    "pages",
    "assets"
  ],
  "exclude": ["node_modules"]
}
